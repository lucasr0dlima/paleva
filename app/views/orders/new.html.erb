<%= form_with model: @dish do |f| %>
  <div>
    <%= f.label :name %> <br />
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :email %> <br />
    <%= f.text_field :email %>
  </div>

  <div>
    <%= f.label :cpf %> (opcional)<br />
    <%= f.text_field :cpf %>
  </div>

  <div>
    <%= f.label :phone_number %> <br />
    <%= f.text_field :phone_number %>
  </div>

  <div class="register_button">
    <%= f.submit "Cadastrar" %>
  </div>

  <% @order_list%>

<% end %>