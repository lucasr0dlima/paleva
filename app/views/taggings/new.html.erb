<h3>Adicionar tag a <%= @product.name %></h3>

<%= form_with model: [@product, @tagging] do |f| %>
  <div>
    <%= f.label :tag_id, "Selecione a Tag" %> <br />
    <%= f.collection_select :tag_id, @tags, :id, :name %>
  </div>

  <div class="register_button">
    <%= f.submit "Adicionar" %>
  </div>
<% end %>